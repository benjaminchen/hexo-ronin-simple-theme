.page-nav {
    font-family: "Roboto", sans-serif;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 20px;
}

.page-nav > span,
.page-nav > a {
    margin: 0 10px;
    color: black;
}

.page-nav > .current {
    color: lightgray;
}

.page-nav > a:hover {
    color: #212529;
}
